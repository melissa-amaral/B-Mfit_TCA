<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<?import javafx.scene.control.ComboBox?>
<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.999-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.control.JanelaCadastroUsuario">

    <center>
        <VBox>
            <Text text="NOME:" />
            <TextField fx:id="tfNome" prefHeight="26.0" prefWidth="388.0" />

            <Text text="RG:" />
            <TextField fx:id="tfRg" prefHeight="26.0" prefWidth="431.0" />

            <Text text="CPF:" />
            <TextField fx:id="tfCPF" />

            <Text text="LOGIN:" />
            <TextField fx:id="tfLogin" />

            <Text text="SENHA:" />
            <TextField fx:id="tfSenha" />

            <Text text="EMAIL:" />
            <TextField fx:id="tfEmail" />

            <Text text="Nivel" />
            <ComboBox fx:id="nivel" />



            <Button onAction="#cadastrar" text="Cadastrar" />
            <Button onAction="#voltar" text="Voltar" />
        </VBox>
    </center>
   <top>

        <HBox BorderPane.alignment="CENTER" />
   </top>

</BorderPane>
